{
  "name": "sio2-bidirectional-sync",
  "version": "1.0.2",
  "description": "A bidirectional folder synchronization tool",
  "main": "dist/src/sync.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/sync.js",
    "dev": "ts-node src/sync.ts",
    "test": "jest",
    "test:watch": "jest --watch",
        "version:patch": "npm version patch -m 'Bump version to %s'",

    "version:minor": "npm version minor -m 'Bump version to %s'",

    "version:major": "npm version major -m 'Bump version to %s'",
    "release": "bun build --compile --minify --outfile sync.exe dist/src/sync.js"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "fs-extra": "^11.1.1",
    "form-data": "^4.0.0",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "ts-node": "^10.9.1",
    "@types/node": "^20.11.16",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.11",
    "supertest": "^6.3.3"
  }
}
